{
  "name": "@mixpeek/prebid-server",
  "version": "1.0.1",
  "type": "module",
  "description": "Mixpeek Server-Side RTD Reference for Prebid Server - Reference implementation and test harness for OpenRTB bid request enrichment with identical field mappings to the Go data provider",
  "main": "dist/index.js",
  "module": "src/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node scripts/build.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:live": "MIXPEEK_API_KEY=$MIXPEEK_API_KEY node --experimental-vm-modules node_modules/jest/bin/jest.js --testMatch='**/tests/live/**/*.test.js'",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testMatch='**/tests/unit/**/*.test.js'",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "prebid",
    "prebid-server",
    "rtd",
    "real-time-data",
    "contextual",
    "advertising",
    "header-bidding",
    "server-side",
    "ssp",
    "cookie-free",
    "privacy",
    "mixpeek",
    "ai",
    "multimodal",
    "iab",
    "taxonomy",
    "brand-safety",
    "adjacency",
    "openrtb"
  ],
  "author": "Mixpeek",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mixpeek/connectors.git",
    "directory": "prebid-server"
  },
  "homepage": "https://github.com/mixpeek/connectors/tree/main/prebid-server#readme",
  "bugs": {
    "url": "https://github.com/mixpeek/connectors/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "jest": "^29.0.0",
    "eslint": "^8.0.0"
  },
  "peerDependencies": {},
  "dependencies": {}
}
